@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}

<h1 class="text-info">Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Username
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody class="text-info">
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.UserName
            </td>
            <td>
                @item.Email
            </td>
        </tr>
}
    </tbody>
</table>
